{
    "name": "leongersen/entity-generator",
    "description": "Database-first Doctrine entity generator with oneToMany and manyToOne support",
    "license": "MIT",
    "bin": ["bin/entity-generator"],
    "require": {
        "php": "^7.1",
        "greenlion/php-sql-parser": "^4.3",
        "nyholm/dsn": "^1.0",
        "symfony/console": "^4.4",
        "symfony/filesystem": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {"EntityGenerator\\": "src/"}
    },
    "config": {
        "sort-packages": true
    }
}
